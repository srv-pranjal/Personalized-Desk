{
  "manifest_version": 2,
  "name": "Personalized Desk",
  "author": "Pranjal Srivastava",
  "version": "1.0.2",
  "description": "A Browser extension for a beautifully simple start page",
  "icons": {
    "16": "/favicon-16x16.png",
    "32": "/favicon-32x32.png",
    "60": "/favicon-60x60.png",
    "72": "/favicon-72x72.png",
    "96": "/favicon-96x96.png"
  },
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
